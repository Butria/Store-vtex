{
  "sticky-layout#sticky__header-desktop": {
    "title": "Sticky Header Desktop",
    "props": {
      "blockClass": "sticky__header-desktop",
      "fullWidth":true
    },
    "children": [
      "flex-layout.row#desktop"
    ]
  },

  "flex-layout.row#desktop": {
    "title": "Container Header Desktop",
    "props": {
      "horizontalAlign": "center",
      "verticalAlign": "center",
      "preventHorizontalStretch": true,
      "preventVerticalStretch": true,
      "fullWidth": false,
      "blockClass": "sticky-header-1"
    },
    "children": [
      "drawer",
      "flex-layout.col#logo-desktop",
      "search-bar#desktop",
      "vtex.menu@2.x:menu#header__one",
      "flex-layout.col#spacer",
      "minicart.v2",
      "login"
    ]
  },
